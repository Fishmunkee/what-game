<h1>My Profile</h1>
<div class="games-list">
  <div>
    <% @user.user_games.each do |user_game| %>
        <% if user_game.completed %>
            <p>Completed <%= user_game.game.title %></p>
        <% end %>
    <% end %> 
  </div>

  <div>
    <% @user.user_games.each do |user_game| %>
        <% if user_game.owned %>
            <p>Owned <%= user_game.game.title %></p>
        <% end %>
    <% end %>
  </div>

  <div>
    <% @user.user_games.each do |user_game| %>
        <% if user_game.wishlist %>
            <p>Wishlist <%= user_game.game.title %></p>
        <% end %>
    <% end %>
  </div>
</div>